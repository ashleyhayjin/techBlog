    {{#each posts as |post|}}

     <div class="card ">
        <h1>{{post.title}}</h1>
            <h3>{{post.words}}</h3>
            <div>
                <p>By: {{post.user.username}}</p>
                <p>{{post.createdAt}}</p>
                <a href="/dashboard/edit/{{post.id}}">Edit</a>
                <a href="/posts/delete">Delete</a>
             
            </div>
    </div>
       
    {{/each}}   



